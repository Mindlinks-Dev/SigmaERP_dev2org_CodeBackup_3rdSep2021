    <fields>
        <fullName>Budget_Acheived__c</fullName>
        <externalId>false</externalId>
        <formula>1  -  IF( Budget_Type__c  = &apos;Budget - Purchase&apos;, ((  Amount__c - Total_Purchased_Amount__c ) / Amount__c ), 
IF( Budget_Type__c  = &apos;Sales Target&apos;, (( Amount__c - Total_Sold_Amount__c ) / Amount__c ), 
(( Amount__c -  Total_Amount_Project__c  ) / Amount__c )
 )
 )</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
