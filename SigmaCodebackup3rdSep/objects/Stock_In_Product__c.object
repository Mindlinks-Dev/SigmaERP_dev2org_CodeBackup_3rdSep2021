    <validationRules>
        <fullName>Check_stock_in_product_quantity</fullName>
        <active>false</active>
        <errorConditionFormula>Available_quantity__c &lt; 0</errorConditionFormula>
        <errorMessage>Quantity Exceeded</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Quantity_matching</fullName>
        <active>false</active>
        <errorConditionFormula>Purchase_Order__r.Received_Quantity__c &lt; Quantity_Received__c</errorConditionFormula>
        <errorMessage>Quantity Received exceeds quantity Ordered</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Received_Quantity_should_be_positive</fullName>
        <active>false</active>
        <errorConditionFormula>Quantity_Received__c &lt; 0</errorConditionFormula>
        <errorDisplayField>Returned_Quantity__c</errorDisplayField>
        <errorMessage>Quantity received is incorrect.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Returned_Quantity_sholud_be_greater_than</fullName>
        <active>false</active>
        <errorConditionFormula>Returned_Quantity__c &lt; 0</errorConditionFormula>
        <errorMessage>Received Quantity is incorrect.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Verified_Products</fullName>
        <active>false</active>
        <errorConditionFormula>ISCHANGED( Quantity_Received__c)  &amp;&amp;    
ISPICKVAL( Status__c , &quot;Verified&quot;) &amp;&amp;  NOT(ISNEW())</errorConditionFormula>
        <errorMessage>This Product is already Verified.</errorMessage>
    </validationRules>
