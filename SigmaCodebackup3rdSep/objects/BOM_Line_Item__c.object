    <validationRules>
        <fullName>CheckStockItemProducts</fullName>
        <active>true</active>
        <description>Added to Restrict Non stok Items as a Line Item for BOM Products.</description>
        <errorConditionFormula>OR(ISBLANK(Product__r.Name),Product__r.Stock_Item__c == false,
Products__r.Stock_Item__c == false)</errorConditionFormula>
        <errorMessage>&quot;Please select any stock item products  to Add Line Item&quot;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validation_for_Quantity</fullName>
        <active>true</active>
        <errorConditionFormula>ISBLANK( Quantity__c )</errorConditionFormula>
        <errorDisplayField>Quantity__c</errorDisplayField>
        <errorMessage>Enter  Quantity</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validation_for_line_item_product</fullName>
        <active>false</active>
        <errorConditionFormula>ISBLANK(Product__r.Name)</errorConditionFormula>
        <errorDisplayField>Product__c</errorDisplayField>
        <errorMessage>Please select product</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Valrestrict_Same_Prod_as_LineItem</fullName>
        <active>true</active>
        <errorConditionFormula>Products__c = Product__c</errorConditionFormula>
        <errorDisplayField>Product__c</errorDisplayField>
        <errorMessage>Bom Product and Line Item Product Should not be same.</errorMessage>
    </validationRules>
