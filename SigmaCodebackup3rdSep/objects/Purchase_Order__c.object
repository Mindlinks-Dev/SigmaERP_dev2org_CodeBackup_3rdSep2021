    </validationRules>
    <validationRules>
        <fullName>Submitted_State</fullName>
        <active>false</active>
        <errorConditionFormula>IF(AND(ISNEW(),ISPICKVAL(Status__c, &apos;Submitted&apos;)), false,  ISPICKVAL(PRIORVALUE(Status__c), &apos;Submitted&apos;) )</errorConditionFormula>
        <errorMessage>Purchase Order is already Submitted.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>vendorStatusValidation</fullName>
        <active>false</active>
        <errorConditionFormula>( OR(AND(
       ISBLANK(Expected_Delivery_Date__c  ),
ISBLANK(Expected_Shipped_Date__c ),
              ISPICKVAL(Vendor_Status__c , &quot;Dispatched&quot;)
   ),AND(
       ISBLANK(Expected_Delivery_Date__c  ),
              ISPICKVAL(Vendor_Status__c , &quot;Dispatched&quot;)
   ),AND(
ISBLANK(Expected_Shipped_Date__c ),
              ISPICKVAL(Vendor_Status__c , &quot;Dispatched&quot;)
   )))</errorConditionFormula>
        <errorMessage>Please select Shipment Date and Expected Delivery Date</errorMessage>
    </validationRules>
