    <validationRules>
        <fullName>Do_not_Allow_Negative_values</fullName>
        <active>true</active>
        <errorConditionFormula>OR( IF( Duty__c &lt; 0, true, false),
 IF( Freight__c &lt; 0, true, false) ,
 IF( Insurance__c &lt; 0, true, false),
 IF( Other__c &lt; 0, true, false),

 IF(  ISNULL(Duty__c )  , true, false) ,
 IF(  ISNULL(Freight__c )  , true, false) ,
 IF(  ISNULL(Insurance__c )  , true, false) ,
 IF(  ISNULL(Other__c )  , true, false) )</errorConditionFormula>
        <errorMessage>Duty, Freight, Insurance or Others charges can not be negative or empty</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validation_for_Empty_Vendor_Name</fullName>
        <active>true</active>
        <description>new vender is updated with empty record</description>
        <errorConditionFormula>ISBLANK( Account_Name__c )</errorConditionFormula>
        <errorDisplayField>Account_Name__c</errorDisplayField>
        <errorMessage>Please select the Vendor name</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validationfor_EmptyLocation</fullName>
        <active>true</active>
        <errorConditionFormula>Vendor_Location__r.Name == &apos;&apos;</errorConditionFormula>
        <errorDisplayField>Vendor_Location__c</errorDisplayField>
        <errorMessage>Configure location to create new vendor product!</errorMessage>
    </validationRules>
